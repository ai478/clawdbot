# Copy to .env and fill with your Twilio credentials
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_auth_token_here
# Must be a WhatsApp-enabled Twilio number, prefixed with whatsapp:
TWILIO_WHATSAPP_FROM=whatsapp:+17343367101

# Supabase Configuration
#
# Option 1: Single Account (backward compatible)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Option 2: Multi-Account (advanced)
# Set SUPABASE_ACCOUNTS as a JSON array with multiple accounts.
# Bash skills can select accounts with:
#   source ~/.clawdbot/skills/supabase/scripts/supabase-api.sh
#   select_supabase_account "production"
#
# SUPABASE_ACCOUNTS='[
#   {
#     "name": "production",
#     "url": "https://prod-project.supabase.co",
#     "key": "prod-service-role-key-here",
#     "anon_key": "prod-anon-key-here",
#     "service_role_key": "prod-service-role-key-here"
#   },
#   {
#     "name": "staging",
#     "url": "https://staging-project.supabase.co",
#     "key": "staging-service-role-key-here",
#     "anon_key": "staging-anon-key-here",
#     "service_role_key": "staging-service-role-key-here"
#   }
# ]'

# MiniMax API Key
MINIMAX_API_KEY=your_minimax_api_key_here


# Copy this file to .env and fill in your actual values
# Docker Compose will automatically load these variables

# Clawdbot Gateway
CLAWDBOT_GATEWAY_TOKEN=your-gateway-token-here
CLAWDBOT_CONFIG_DIR=/home/clawdis/.clawdbot
CLAWDBOT_WORKSPACE_DIR=/home/clawdis/clawdbot

# Telegram Bot
TELEGRAM_BOT_TOKEN=your-telegram-bot-token-here

# Brave Search API
BRAVE_API_KEY=your-brave-api-key-here
GITHUB_TOKEN=your-github-token-here

# Optional: Claude AI (if using)
CLAUDE_AI_SESSION_KEY=
CLAUDE_WEB_SESSION_KEY=
CLAUDE_WEB_COOKIE=

# Optional: Other services
GOOGLE_PLACES_API_KEY=
GEMINI_API_KEY=
OPENAI_API_KEY=
ELEVENLABS_API_KEY=

# Codex Callback Configuration
# Default agent to wake when codex completes (admin or team)
CLAWDBOT_DEFAULT_AGENT=admin
# Gateway URL for wake commands
CLAWDBOT_GATEWAY_URL=http://localhost:18789
